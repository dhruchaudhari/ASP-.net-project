@model CuisineDTO
@{
    Layout = "_AdminLayout";
}
@* @Html.ValidationSummary() *@
<div style="width:50%">
<h2> Update Cuisine </h2>
<a asp-action="Index" asp-controller="Cuisine" class="btn btn-primary">Back</a>
<form asp-action="UpdateCuisine">
   <input type="hidden" asp-for="Id" />

   <div class="my-2">
      <label>Cuisine</label>
      <input type="text" class="form-control" asp-for="CuisineName" />
            <span asp-validation-for="CuisineName" class="text-danger"></span>
   </div>

   <div class="my-2">
      <button type="submit" class="btn btn-info">Update</button>
   </div>
</form>
</div>
